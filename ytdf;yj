x=input('Назови один любой месяц')
if x =='май' or x== 'март' or x=='апрель':
    print('весна')
elif x =='февраль' or x== 'декабрь' or x == 'январь':
    print('зима')
elif x =='июнь' or x== 'июль' or x == 'август':
    print('лето')
elif x =='сентябрь' or x == 'октябрь' or x == 'ноябрь':
    print('осень')
