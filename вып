l=["сайт,программа,борщ"]
print("привет, я бот Воки")
print("чтобы начть игру тебе надо написать,играть,вот и всё")
s=input()
if s == "играть":
    print("в этой игре тебе нужно будет выбрать лишнее слово,просто напиши лишнее,с начало будет просто но потом... сам увидишь")
    print(l)
    s=input()
    if s!= "борщ":
        print("неправильно")
        print(l)
        s=input()   
    s=input()
    if s== "борщ":
        print("правильно")
        l=['вор,вода,водяной']
        print(l)
        s = input()
        if s!= "водяной":
            print("неправильно")
            print(l)
            s=input()
        s= input()
        if s == 'водяной':
            print('правильно')
print("конец")

